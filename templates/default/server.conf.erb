# OpenVPN server configuration file
# Auto-generated by Chef, local changes will be overwritten on the next chef run.

<% @config.sort.each do |key, value| %>
<% next if value.nil? -%>
<% if value.is_a?(Array) %>
  <% value.each do |item| %>
<%= key %> '<%= item %>'
  <% end %>
  <% next %>
<% end %>
<%= key %> <%=
  case value
  when String
    value
  when TrueClass
    1
  when FalseClass
    0
  else
    value
  end
%>
<% end %>
